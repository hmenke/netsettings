{ config, lib, pkgs, ... }:

{
  userPackages = with pkgs; [
    browserpass
    (chromium.override {
      commandLineArgs = [
        "--disk-cache-dir=/dev/shm/$USER/cache/chromium"
        "--disable-gpu-shader-disk-cache"
        "--enable-features=WebRTCPipeWireCapturer,VaapiVideoDecoder,Vulkan"
        "--enable-gpu-rasterization"
        "--enable-zero-copy"
        "--use-vulkan"
      ];
    })
    element-desktop
    evince
    evolution
    gimp
    gnomeExtensions.appindicator
    (gnomeExtensions.bing-wallpaper-changer.override {
      version = "43";
      sha256 = "1bhd491i0kzn8ia55wgrw99jvz5jrjxhxq9bf44cw542pica5zkb";
      metadata = "";
    })
    gnomeExtensions.gsconnect
    gnomeExtensions.pop-shell
    gnomeExtensions.sound-output-device-chooser
    libnotify
    onlyoffice-bin
    strawberry
    virt-manager
    vlc
    wl-clipboard
    wofi
    xournalpp
  ];
}
